				<string>import sys
import os
import subprocess
from workflow import Workflow

def main(wf):
	if '{query}' == 'help':
		annotation = wf.workflowfile('skimmer_config_new_embed.pdf')
    		if os.path.isfile(annotation):
        		subprocess.Popen(['open', annotation], stdout=subprocess.PIPE)
    		else:
        		return annotation
	elif '{query}' == 'highlight':
		scpt = wf.workflowfile('set_highlight.scpt')
		process = subprocess.Popen(['osascript', scpt],
                                    stdout=subprocess.PIPE)
        	out = process.communicate()[0]
        	print out.strip()
	elif '{query}' == 'handler':
		handler = wf.workflowfile('_skimmer.app')
		process = subprocess.Popen(['open', handler],
                                    stdout=subprocess.PIPE)
        	out = process.communicate()[0]

if __name__ == '__main__':
    wf = Workflow()
    sys.exit(wf.run(main))</string>
