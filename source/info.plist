				<string>#!/usr/bin/python
# encoding: utf-8
import sys
import os.path
from workflow import Workflow

def main(wf):
	wf.add_item(u"Open PDF", u"Open Skimmer's Help PDF?", valid=True, arg='help', icon='icons/n_pdf.png')
	wf.add_item(u"Set Highlights", u"Set Highlight Settings", valid=True, arg='highlight', icon='icons/n_folder.png') 
	wf.add_item(u"Configure Skimmer URL Handler", u"Open the URL handler app to register Skimmer's custom URL", valid=True, arg='handler', icon='icons/n_folder.png') 

	wf.send_feedback()
		
if __name__ == '__main__':
	wf = Workflow()
	sys.exit(wf.run(main))</string>
